# 高通ATH79平台专用配置

# ==================== USB 专用驱动 ====================
# 🔧 ATH79 USB控制器
CONFIG_PACKAGE_kmod-usb2-ath79=y
CONFIG_PACKAGE_kmod-usb-ohci=y

# ==================== 无线驱动 ====================
# 📡 ATH9K 无线驱动
CONFIG_PACKAGE_kmod-ath9k=y
CONFIG_PACKAGE_kmod-ath9k-common=y
CONFIG_PACKAGE_kmod-ath9k-htc=y
CONFIG_PACKAGE_ath9k-htc-firmware=y

# ==================== GPIO支持 ====================
CONFIG_PACKAGE_kmod-ath79-gpio-buttons=y
CONFIG_PACKAGE_kmod-ath79-gpio-leds=y
