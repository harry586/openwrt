# 联发科平台专用配置

# ==================== USB 专用驱动 ====================
# 🔧 联发科DWC3 USB控制器
CONFIG_PACKAGE_kmod-usb-dwc3-mediatek=y
CONFIG_PACKAGE_kmod-phy-mediatek=y

# ==================== 无线驱动 ====================
# 📡 MT79xx 无线驱动
CONFIG_PACKAGE_kmod-mt7915e=y
CONFIG_PACKAGE_mt7915-firmware=y
CONFIG_PACKAGE_kmod-mt798x-firmware=y

# ==================== 网络加速 ====================
# ⚡ 硬件NAT加速
CONFIG_PACKAGE_kmod-mtk-hwnat=y
CONFIG_PACKAGE_kmod-mtk-ppe=y
